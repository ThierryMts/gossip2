<ul>
    <% if user_signed_in? %>
    <li>
        <%= link_to "Signed Out", destroy_user_session_path, method: :delete %>
    </li>
    <li>
        <%= current_user.email %>
    </li>
    <li>
        <%= link_to "Edit account", edit_user_registration_path %>
    </li>
    <% else %>
    <li>
        <%= link_to "sign in", new_user_session_path %>
    </li>
    <li>
        <%= link_to "sign up", new_user_registration_path %>
    </li>
    <% end %>
</ul>